        },
        {
          "name": "role",
          "description": "Role",
          "type": "Role"
        },

        {
          "name": "user_name",
          "description": "User name",
          "type": "string"
        },
        {
          "name": "email",
          "description": "User email",
          "type": "string"
        },
        {
          "name": "password",
          "description": "User password",
          "type": "string"
        }
      ]
    }
  ]
}
